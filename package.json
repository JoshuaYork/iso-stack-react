{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "description": "An isomorphic React application implementing latest development standards",
  "main": "index.js",
  "scripts": {
    "build": "cross-env webpack --config ./webpack.config.prod.babel.js",
    "postinstall": "npm install -g babel-cli@7.0.0-beta.3 cross-env@5.0.5 webpack@3.6.0",
    "start": "npm run start-prod",
    "start-dev": "cross-env NODE_ENV=development babel-node server --useServerRender=true --useLiveData=false --inspect",
    "start-prod": "npm run build && cross-env NODE_ENV=production babel-node server --useServerRender=true --useLiveData=true",
    "start-dev-live": "cross-env NODE_ENV=development babel-node server --useServerRender=true --useLiveData=true",
    "start-dev-live-debug": "cross-env NODE_ENV=development babel-node server --useServerRender=true --useLiveData=true --inspect"
  },
  "keywords": [],
  "author": "Josh York",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-regenerator-runtime": "^6.5.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "express-yields": "^1.1.2",
    "fs-extra": "^4.0.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "optimist": "^0.6.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "query-string": "^6.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-server": "^4.2.2",
    "react-styleable": "^3.3.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.0"
  }
}
